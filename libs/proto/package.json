{
  "name": "@nightingale/proto",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@bufbuild/protobuf": "2.3.0",
    "@grpc/grpc-js": "^1.13.3",
    "tslib": "^2.8.1"
  },
  "nx": {
    "targets": {
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./libs/proto/src/generated --ts_proto_opt=outputServices=grpc-js,useOptionals=messages -I libs/proto/src libs/proto/src/*.proto"
          ],
          "parallel": false
        }
      }
    }
  }
}